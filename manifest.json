{
  "name": "Happy Birthday Surprise",
  "description": "Install this for you friend on their birthday for a surprise!",
  "version": "1.0.0",
  "author": "Connor Haines",
  "manifest_version": 3,
  "action": {
    "default_popup": "birthdayLinks.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["styles.css"]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
